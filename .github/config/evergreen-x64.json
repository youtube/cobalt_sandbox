{
  "docker_service": "build-linux-evergreen",
  "bootloader": "true",
  "on_host_test": "true",
  "on_host_test_shards": ["unit", "integration"],
  "platforms": [
    "evergreen-x64",
    "evergreen-x64-sbversion-12"
  ],
  "includes": [
    {
      "name":"x64",
      "platform":"evergreen-x64",
      "target_platform":"evergreen-x64",
      "bootloader_platform": "linux-x64x11"
    },
    {
      "name":"sbversion-12",
      "platform":"evergreen-x64-sbversion-12",
      "target_platform":"evergreen-x64",
      "bootloader_platform":"linux-x64x11-sbversion-12"
    }
  ]
}
